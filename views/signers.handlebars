<div class="main-content">
<h2>The list of participants are...</h2>
<ol id="signer-names">
{{#signers}}
<li>{{first}} {{last}}
    {{#if age}}, {{age}}{{/if}}
    {{#if city}}, <a href="/signers/{{city}}">{{city}}</a>{{/if}}
    {{#if url}}, <a href="{{url}}">Website</a>{{/if}}
</li>
{{/signers}}
</ol>
</div>